    "@types/node": "^22.13.5",
    "@typescript-eslint/parser": "^8.24.1",
    "@typescript-eslint/rule-tester": "^8.24.1",
    "@typescript-eslint/utils": "^8.24.1",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
